		"source": "https://github.com/le-o/yii2-dynamicform"
	},
	"authors": [
		{
			"name": "le-o",
			"email": "leo@powman.pro"
		}
	],
	"require": {
		"yiisoft/yii2": "*",
		"symfony/dom-crawler": "*",
		"symfony/css-selector": "*"
